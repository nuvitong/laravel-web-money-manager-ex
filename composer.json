{
    "name": "okaufmann/laravel-web-money-manager-ex",
    "description": "Complete rewrite of Money Manager Ex WebApp based on Laravel for better security and more functionality.",
    "keywords": [
        "framework",
        "laravel",
        "mmex",
        "money",
        "webapp"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.0",
        "doctrine/dbal": "~2.3",
        "laravel/framework": "5.5.*",
        "mariuzzo/laravel-js-localization": "^1.4",
        "okaufmann/laravel-git-version": "^1.1",
        "rap2hpoutre/laravel-log-viewer": "^0.10",
        "sentry/sentry-laravel": "^0.8.0",
        "spatie/laravel-blade-javascript": "^2.0",
        "spatie/laravel-fractal": "^5.0",
        "spatie/laravel-medialibrary": "^6.0",
        "webpatser/laravel-uuid": "^3.0",
        "fideloper/proxy": "~3.3"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "barryvdh/laravel-debugbar": "^3.0",
        "barryvdh/laravel-ide-helper": "^2.3",
        "fzaninotto/faker": "~1.4",
        "laravel/browser-kit-testing": "^2.0",
        "laravel/tinker": "^1.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "~6.3"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "Tests\\": "tests/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/tinker",
                "barryvdh/laravel-debugbar",
                "barryvdh/laravel-ide-helper"
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-update-cmd": [
            "@php artisan package:discover",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}